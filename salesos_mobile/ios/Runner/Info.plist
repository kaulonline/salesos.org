<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>$(DEVELOPMENT_LANGUAGE)</string>
	<key>CFBundleDisplayName</key>
	<string>SalesOS</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>SalesOS</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>$(FLUTTER_BUILD_NAME)</string>
	<key>CFBundleSignature</key>
	<string>????</string>
	<key>CFBundleVersion</key>
	<string>$(FLUTTER_BUILD_NUMBER)</string>
	<key>LSRequiresIPhoneOS</key>
	<true/>
	<key>UILaunchStoryboardName</key>
	<string>LaunchScreen</string>
	<key>UIMainStoryboardFile</key>
	<string>Main</string>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>UISupportedInterfaceOrientations~ipad</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>CADisableMinimumFrameDurationOnPhone</key>
	<true/>
	<key>UIApplicationSupportsIndirectInputEvents</key>
	<true/>

	<!-- Export Compliance - App uses standard HTTPS encryption only -->
	<key>ITSAppUsesNonExemptEncryption</key>
	<false/>

	<!-- Privacy Permissions -->
	<key>NSCameraUsageDescription</key>
	<string>SalesOS needs camera access to scan business cards and documents</string>
	<key>NSPhotoLibraryUsageDescription</key>
	<string>SalesOS needs photo library access to attach images to records</string>
	<key>NSPhotoLibraryAddUsageDescription</key>
	<string>SalesOS needs permission to save images to your photo library</string>
	<key>NSMicrophoneUsageDescription</key>
	<string>SalesOS needs microphone access for voice commands and call recording</string>
	<key>NSSpeechRecognitionUsageDescription</key>
	<string>SalesOS uses speech recognition for voice-to-text features</string>
	<key>NSLocationWhenInUseUsageDescription</key>
	<string>SalesOS uses your location to log check-ins and find nearby contacts</string>
	<key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
	<string>SalesOS uses your location for geo-fencing and location-based reminders</string>
	<key>NSFaceIDUsageDescription</key>
	<string>SalesOS uses Face ID for secure authentication</string>
	<key>NSContactsUsageDescription</key>
	<string>SalesOS can sync with your contacts to help manage relationships</string>
	<key>NSCalendarsUsageDescription</key>
	<string>SalesOS can sync meetings with your calendar</string>
	<key>NSCalendarsWriteOnlyAccessUsageDescription</key>
	<string>SalesOS can add meetings to your calendar</string>
	<key>NSLocalNetworkUsageDescription</key>
	<string>SalesOS uses local network for real-time voice communication</string>

	<!-- Background Modes -->
	<key>UIBackgroundModes</key>
	<array>
		<string>audio</string>
		<string>fetch</string>
		<string>remote-notification</string>
	</array>

	<!-- Supports multiple scenes -->
	<key>UIApplicationSceneManifest</key>
	<dict>
		<key>UIApplicationSupportsMultipleScenes</key>
		<false/>
	</dict>

	<!-- App Transport Security - Production Ready -->
	<key>NSAppTransportSecurity</key>
	<dict>
		<key>NSAllowsArbitraryLoads</key>
		<false/>
		<key>NSExceptionDomains</key>
		<dict>
			<key>salesos.org</key>
			<dict>
				<key>NSExceptionAllowsInsecureHTTPLoads</key>
				<false/>
				<key>NSIncludesSubdomains</key>
				<true/>
			</dict>
		</dict>
	</dict>

	<!-- Firebase Crashlytics -->
	<key>FirebaseCrashlyticsCollectionEnabled</key>
	<true/>

	<!-- Required Privacy Manifest -->
	<key>NSPrivacyAccessedAPITypes</key>
	<array>
		<dict>
			<key>NSPrivacyAccessedAPIType</key>
			<string>NSPrivacyAccessedAPICategoryUserDefaults</string>
			<key>NSPrivacyAccessedAPITypeReasons</key>
			<array>
				<string>CA92.1</string>
			</array>
		</dict>
	</array>
</dict>
</plist>
